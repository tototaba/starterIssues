  "name": "univerus-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=6144' env-cmd -f .env react-scripts build",
    "build:demo": "cross-env NODE_OPTIONS='--max-old-space-size=6144' env-cmd -f .env.demo react-scripts build",
    "build:development": "cross-env NODE_OPTIONS='--max-old-space-size=6144' env-cmd -f .env.development react-scripts build",
    "build:staging": "cross-env NODE_OPTIONS='--max-old-space-size=6144' env-cmd -f .env.staging react-scripts build",
    "build:production": "cross-env NODE_OPTIONS='--max-old-space-size=6144' env-cmd -f .env.production react-scripts build",
    "eject": "react-scripts eject",
    "preinstall": "node yarn-required.js",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx .",
    "pretty": "prettier --write .",
    "start": "env-cmd -f .env react-scripts start",
    "start:demo": "env-cmd -f .env.demo react-scripts start",
    "start:development": "env-cmd -f .env.development react-scripts start",
    "start:staging": "env-cmd -f .env.staging react-scripts start",
    "start:production": "env-cmd -f .env.production react-scripts start",
    "test": "react-scripts test",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "chromatic": "npx chromatic --project-token a0a37fb3ac99",
    "server": "nodemon --watch 'src/api/server/**/*.ts' --exec ts-node -- -P tsconfig.server.json src/api/server/index.ts",
    "generate-api-spec": "env-cmd -f .env.development -x openapi-typescript \\$WORK_MANAGEMENT_API_DOMAIN/swagger/v1/swagger.json --output src/api/swaggerSpec.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && license-validator"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --ext .js,.jsx,.ts,.tsx"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
